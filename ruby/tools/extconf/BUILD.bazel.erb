package(default_visibility = ["//visibility:public"])

cc_library(
    name = "<%= name %>",
    srcs = [
      <%- srcs.each do |src| -%>
        <%= src.to_s %>,
      <%- end -%>
      <%- hdrs.each do |src| -%>
        <%= src.to_s %>,
      <%- end -%>
    ],
    copts = [
      <%- copts.each do |flag| -%>
        <%= flag.to_s %>,
      <%- end -%>
    ],
    linkopts = [
      <%- ldflags.each do |flag| -%>
        <%= flag.to_s %>,
      <%- end -%>
    ],
    deps = [
      <%- deps.each do |dep| -%>
        <%= dep.to_s %>,
      <%- end -%>
    ],
)
# vim: set ft=bzl :
